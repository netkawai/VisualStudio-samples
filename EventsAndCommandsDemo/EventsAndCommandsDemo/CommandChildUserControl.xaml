<UserControl x:Class="EventsAndCommandsDemo.CommandChildUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="174" Width="233">
    <UserControl.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Save" CanExecute="OnCanExecute" Executed="OnExecute" />
    </UserControl.CommandBindings>
    <Grid Height="154" Width="218">
        <TextBox Height="23" Margin="25,18,0,0" Name="textBox1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120" />
        <TextBox Height="23" Margin="25,61,0,0" Name="textBox2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120" />
        <Button Command="ApplicationCommands.Save" Height="23" Margin="69,95,0,0" Name="button1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="76">Save</Button>
    </Grid>
</UserControl>
